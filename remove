#include<stdio.h>
int main()
{
    char str[100],str2[100];
    int ch,t,h,e,spc,count=0,j;
    printf("Enter string");
    fgets(str, sizeof str , stdin);
    int i;
    for(i=0,j=0;str[i]!='\0';i++)
    {
        if((str[i]>='a' && str[i]<='z') ||(str[i]>='A' && str[i]<='Z'))
        {
            str2[j]=str[i];
            j++;
        }
    }
    str2[j]='\0';
    printf("string is %s",str2);
}
