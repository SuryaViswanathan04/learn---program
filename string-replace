#include <stdio.h>
#include<string.h>
int main()
{
    char str[100];
    char ch[100]="is";
    int count=0;
    fgets(str,sizeof str,stdin);
    int i;
    for(i=0;str[i]!='\0';i++)
    {
      
        if(str[i]=='t' && str[i+1]=='h' &&str[i+2]=='e')
        {
           
           str[i]='m';
           str[i+1]='m';
           str[i+2]='m';
            
        }
    }
    printf("str is %s",str);
}
