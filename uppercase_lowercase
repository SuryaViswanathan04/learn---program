#include <stdio.h>
#include<string.h>
#include <ctype.h>
int main()
{
    char a[100];
    int ch;
    printf("enter string");
    fgets(a,sizeof a, stdin);
    int i,j;
    for(i=0; a[i]!='\0';i++)
    {
        ch =islower(a[i]) ? toupper(a[i]) : tolower(a[i]);
        putchar(ch);
        
    }
}
